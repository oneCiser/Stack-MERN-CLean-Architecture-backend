    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.6",
    "multer": "^1.4.4",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "redis": "^4.0.4",
    "ts-node": "^10.4.0",
    "unique-names-generator": "^4.6.0",
    "uuid": "^8.3.2"
