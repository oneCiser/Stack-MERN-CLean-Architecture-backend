    "@types/uuid": "^8.3.4",
    "concurrently": "^7.0.0",
    "jest": "^27.4.7",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.6.2"
