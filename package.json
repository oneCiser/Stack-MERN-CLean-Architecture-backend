{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend component",
  "main": "index.js",
  "engines": {
    "node": "17.3.0"
  },
  "scripts": {
    "test:clear": "jest --clearCache",
    "test:watch": "jest --watch",
    "test": "jest",
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "start": "npm run build && cross-env NODE_ENV=production node build/src/index.js",
